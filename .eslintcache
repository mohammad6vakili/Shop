   {cartItems.length!==0 && \n                    <div>\n                        <div className='payment-section'>\n                            <div className='total'>\n                                <div>\n                                    Total: {\"  $ \"}\n                                    { cartItems.reduce((a,c)=>a+c.price*c.count,0).toFixed(2)}\n                                </div>\n                                <button onClick={showCheckoutForm} className='payment-button'>Proceed</button>\n                            </div>\n                        </div>\n                        <Fade right cascade>\n                        {showCheckout && \n                            <form className='mt-5' onSubmit={createOrder}>\n                                <ul className='form-container'>\n                                    <li>\n                                        <label htmlFor=\"name\">Name</label>\n                                        <input type=\"text\" name='name' value={name} onChange={getName} required/>\n                                    </li>\n                                    <li>\n                                        <label htmlFor=\"email\">Email</label>\n                                        <input type=\"email\" name='email' value={email} onChange={getEmail} required/>\n                                    </li>\n                                    <li>\n                                        <label htmlFor=\"address\">Address</label>\n                                        <input type=\"text\" name='address' value={address} onChange={getAddress} required/>\n                                    </li>\n                                    <li>\n                                        <button className='checkout-button' type='submit'>Checkout</button>\n                                    </li>\n                                </ul>\n                            </form>\n                        }\n                        </Fade>\n                    </div>\n                    }\n                    \n         </div>\n       \n    )\n\n}\nexport default Cart;","/media/mohammad/New Volume/Programming/VsCode/Practices/React/Shop/src/components/products.jsx",["66"],"/media/mohammad/New Volume/Programming/VsCode/Practices/React/Shop/src/components/Filter.jsx",[],{"ruleId":"67","replacedBy":"68"},{"ruleId":"69","replacedBy":"70"},{"ruleId":"71","severity":1,"message":"72","line":1,"column":15,"nodeType":"73","messageId":"74","endLine":1,"endColumn":25},{"ruleId":"71","severity":1,"message":"75","line":1,"column":26,"nodeType":"73","messageId":"74","endLine":1,"endColumn":35},{"ruleId":"71","severity":1,"message":"76","line":2,"column":8,"nodeType":"73","messageId":"74","endLine":2,"endColumn":12},{"ruleId":"71","severity":1,"message":"77","line":11,"column":9,"nodeType":"73","messageId":"74","endLine":11,"endColumn":19},{"ruleId":"67","replacedBy":"78"},{"ruleId":"69","replacedBy":"79"},{"ruleId":"71","severity":1,"message":"72","line":1,"column":15,"nodeType":"73","messageId":"74","endLine":1,"endColumn":25},{"ruleId":"71","severity":1,"message":"75","line":1,"column":26,"nodeType":"73","messageId":"74","endLine":1,"endColumn":35},{"ruleId":"80","severity":1,"message":"81","line":25,"column":42,"nodeType":"82","messageId":"83","endLine":25,"endColumn":43},{"ruleId":"80","severity":1,"message":"81","line":27,"column":42,"nodeType":"82","messageId":"83","endLine":27,"endColumn":43},{"ruleId":"71","severity":1,"message":"84","line":8,"column":8,"nodeType":"73","messageId":"74","endLine":8,"endColumn":12},{"ruleId":"85","severity":1,"message":"86","line":28,"column":12,"nodeType":"87","endLine":28,"endColumn":14,"suggestions":"88"},{"ruleId":"71","severity":1,"message":"89","line":1,"column":8,"nodeType":"73","messageId":"74","endLine":1,"endColumn":13},{"ruleId":"71","severity":1,"message":"90","line":14,"column":9,"nodeType":"73","messageId":"74","endLine":14,"endColumn":21},{"ruleId":"91","severi